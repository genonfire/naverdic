{
  "manifest_version": 2,

  "name": "네이버 사전",
  "description": "웹페이지에서 영어 단어에 더블클릭하면 네이버 사전을 통해 단어의 뜻을 보여줍니다.",
  "version": "0.1",
    "icons": {
      "128": "icon.png",
      "48": "icon.png",
      "16": "icon.png"
   },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://endic.naver.com/",
    "http://dic.dn.naver.com/"
  ],
  "manifest_version": 2
}
